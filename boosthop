repeat
    wait()
until game:IsLoaded()
if game.PlaceId == 6069993731 then
    wait(3)
    local function plramount()
        local Players = game:GetService("Players")
        local e = #Players:GetPlayers()
        if e == 1 then
            print("stfu")
            wait()
        elseif e > 1 then
            game:GetService("StarterGui"):SetCore(
                "PromptBlockPlayer",
                game.Players:GetPlayers()[math.random(2, #game.Players:GetPlayers())]
            )
            wait(1.6)
            game:GetService("TeleportService"):Teleport(705844193, game.Players.LocalPlayer)
        end
    end
	spawn(function()
    while true do
        plramount()
        wait()
    end
	end)
    game:GetService "Players".PlayerAdded:Connect(
        function()
            for _, v in pairs(game:GetService("Players"):GetPlayers()) do
                wait()
                game:GetService("StarterGui"):SetCore("PromptBlockPlayer", v)
            end
            wait(1.4)
            game:GetService("TeleportService"):Teleport(705844193, game.Players.LocalPlayer)
        end
    )
    function notify(text)
        game.StarterGui:SetCore(
            "SendNotification",
            {
                Title = "~IMPORTANT~",
                Text = text,
                Icon = "",
                Duration = 100000
            }
        )
    end
    wait()
    notify("This autohops whenever a person joins, make sure to have your block list at 0 before starting the script")
    for i, b in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
        if b.Name == "EagleVision" and b:IsA("Sound") and b.Parent.Whispers:IsA("Sound") then
            local Event = game:GetService("Players").LocalPlayer.PlayerGui[b.Parent.Name].use
            Event:FireServer()
            wait()
        end
    end
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-5420.25, 32.33, 1616.62)
    wait(3)
    spawn(
        function()
            while true do
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame =
                    CFrame.new(-5420.25, 30, 1616.62)
                wait(1)
                local Tool = "Meat"
                game.Players[game.Players.LocalPlayer.Name].Character.Humanoid:EquipTool(
                    game.Players[game.Players.LocalPlayer.Name].Backpack[Tool]
                )
                game.Workspace[game.Players.LocalPlayer.Name]:FindFirstChild("Meat"):Activate()
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame =
                    CFrame.new(-5416.97, 32.01, 1629.40)
                wait(2)
                for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                    if v.Name == "Armament" and v:IsA("LocalScript") then
                        local string_1 = "Down"
                        local Target = game:GetService("Players").LocalPlayer.PlayerGui[v.Parent.Name].use
                        Target:FireServer(string_1)
                        wait(1)
                        local string_1 = "Down"
                        local Target = game:GetService("Players").LocalPlayer.PlayerGui[v.Parent.Name].use
                        Target:FireServer(string_1)
                        wait(6.8)
                    end
                end
            end
        end
    )
else
    for i = 1, 3 do
        function notify(text)
            game.StarterGui:SetCore(
                "SendNotification",
                {
                    Title = "~IMPORTANT~",
                    Text = text,
                    Icon = "",
                    Duration = 100000
                }
            )
        end
        wait()
        notify("PLACE UR MOUSE WHERE THE BLOCK BUTTON IS WHEN THE GUI POPS UP AND AUTOCLICK")
    end
    for _, o in pairs(game:GetService("Players"):GetPlayers()) do
        wait()
        game:GetService("StarterGui"):SetCore("PromptBlockPlayer", o)
    end
    wait(3.7)
    game:GetService("TeleportService"):Teleport(6069993731, game.Players.LocalPlayer)
end
